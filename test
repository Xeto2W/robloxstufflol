-- Gui to Lua
-- Version: 3.2

-- Instances:

local fetest = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextButton = Instance.new("TextButton")
local TextButton_2 = Instance.new("TextButton")

--Properties:

fetest.Name = "fetest"
fetest.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
fetest.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = fetest
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderColor3 = Color3.fromRGB(255, 0, 0)
Frame.BorderSizePixel = 2
Frame.Position = UDim2.new(0.260428399, 0, 0.275510192, 0)
Frame.Size = UDim2.new(0, 202, 0, 124)

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TextButton.BorderColor3 = Color3.fromRGB(255, 0, 0)
TextButton.BorderSizePixel = 2
TextButton.Position = UDim2.new(0.103960395, 0, 0, 0)
TextButton.Size = UDim2.new(0, 159, 0, 50)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "message test"
TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton.TextSize = 14.000

TextButton_2.Parent = Frame
TextButton_2.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TextButton_2.BorderColor3 = Color3.fromRGB(255, 0, 0)
TextButton_2.BorderSizePixel = 2
TextButton_2.Position = UDim2.new(0.103960395, 0, 0.59677422, 0)
TextButton_2.Size = UDim2.new(0, 159, 0, 50)
TextButton_2.Font = Enum.Font.SourceSans
TextButton_2.Text = "mass rep**t"
TextButton_2.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton_2.TextSize = 14.000

-- Scripts:

local function WLVIZLD_fake_script() -- Frame.drag 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
end
coroutine.wrap(WLVIZLD_fake_script)()
local function TFRWIC_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		function a(a, _)
			pcall(function()
				b = Instance.new("Message", a)
				b.Text = _
				wait(5)
				b:Remove()
			end)
		end
		spawn(function()
			for _, v in pairs(game.Players:GetPlayers()) do
				a(game.CoreGui, "funny")
			end
		end)
	end)
end
coroutine.wrap(TFRWIC_fake_script)()
local function KGYOTGZ_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		for i, speaker in pairs(game.Players:GetPlayers()) do
			if speaker.Name ~= game.Players.LocalPlayer.Name then
				local players = game.Players
	
				while wait() do
					players:ReportAbuse(players[speaker.Name],"Swearing",'He is breaking roblox TOS')
					wait()
					players:ReportAbuse(players[speaker.Name],"Swearing",'He said bad words')
					wait()
					players:ReportAbuse(players[speaker.Name],"Bullying",'He bullied me')
					wait()
					players:ReportAbuse(players[speaker.Name],"Bullying",'He bullied my friend')
					wait()
					players:ReportAbuse(players[speaker.Name],"Scamming",'He scammed me')
					wait()
					players:ReportAbuse(players[speaker.Name],"Scamming",'He is sending scam links')
					wait()
					players:ReportAbuse(players[speaker.Name],"Dating",'He is dating in another game')
					wait()
					players:ReportAbuse(players[speaker.Name],"Dating",'He is dating another player in this game')
				end
			end
		end
	end)
end
coroutine.wrap(KGYOTGZ_fake_script)()
local function SCXQZ_fake_script() -- fetest.LocalScript 
	local script = Instance.new('LocalScript', fetest)

	for _, v in pairs(game:GetDescendants()) do
		if v.Name == "fetest" and v ~= script.Parent then
			v:Destroy()
		end
	end
end
coroutine.wrap(SCXQZ_fake_script)()
